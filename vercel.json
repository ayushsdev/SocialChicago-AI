{
    "version": 2,
    "builds": [
        {
            "src": "api/app.py",
            "use": "@vercel/python",
            "config": {
                "buildCommand": "apt-get update && apt-get install -y poppler-utils"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/app.py"
        }
    ]
}